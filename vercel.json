{
    "crons": [
        {
            "path": "/api/check-expiry",
            "schedule": "0 6 * * *"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/index.js"
        },
        {
            "source": "/OneSignalSDKWorker.js",
            "destination": "/OneSignalSDKWorker.js"
        },
        {
            "source": "/OneSignalSDKUpdaterWorker.js",
            "destination": "/OneSignalSDKUpdaterWorker.js"
        },
        {
            "source": "/OneSignalSDK.sw.js",
            "destination": "/OneSignalSDK.sw.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(OneSignalSDKWorker|OneSignalSDKUpdaterWorker|OneSignalSDK.sw).js",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/javascript"
                }
            ]
        }
    ]
}