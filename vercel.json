{
    "rewrites": [
        {
            "handle": "filesystem"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(OneSignalSDKWorker|OneSignalSDKUpdaterWorker|OneSignalSDK.sw).js",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/javascript"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=0, must-revalidate"
                }
            ]
        }
    ]
}